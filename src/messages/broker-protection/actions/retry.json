{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "required": ["maxAttempts", "interval"],
  "properties": {
    "environment": {
      "type": "string",
      "enum": ["web", "native"]
    },
    "maxAttempts": {
      "type": "number"
    },
    "interval": {
      "type": "object",
      "required": ["ms"],
      "properties": {
