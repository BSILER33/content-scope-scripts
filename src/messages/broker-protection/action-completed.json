            "actionType": {
              "$ref": "./actions/shared.json#/definitions/ActionType"
            },
            "actionID": {
              "type": "string"
            },
            "response": {
              "oneOf": [
                { "$ref": "./actions/navigate-action.json#/definitions/Response" },
                { "$ref": "./actions/extract-action.json#/definitions/Response" }
