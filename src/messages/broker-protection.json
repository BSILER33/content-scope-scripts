{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "BrokerProtectionMessages",
  "description": "Requests, Notifications and Subscriptions from the Broker Protection feature",
  "additionalProperties": false,
  "properties": {
    "notifications": {
      "oneOf": [
        {
          "$ref": "./broker-protection/action-completed.json"
        }
      ]
    },
    "requests": {},
    "subscriptions": {
      "oneOf": [
        {
          "$ref": "./broker-protection/on-action-received.json"
        }
      ]
    }
  },
  "required": [
    "requests",
    "notifications",
    "subscriptions"
  ]
}
